{
  "name": "trailrun",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "deploy:preview": "npm run build:preview",
    "deploy:production": "npm run build:production && npm run submit:ios && npm run submit:android",
    "security:audit": "npm audit && npx audit-ci --config audit-ci.json",
    "prebuild": "expo prebuild --clean",
    "postinstall": "expo install --fix"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.5.10",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@rnmapbox/maps": "^10.1.44",
    "@sentry/react-native": "^7.0.1",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.7",
    "expo-background-fetch": "~13.0.1",
    "expo-camera": "~17.0.7",
    "expo-file-system": "~19.0.14",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-location": "~18.0.4",
    "expo-media-library": "~18.1.1",
    "expo-modules-core": "~1.12.26",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~12.0.2",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-sqlite-2": "^3.6.2",
    "react-native-sqlite-storage": "^6.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.80.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
