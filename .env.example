# TrailRun Environment Configuration
# Copy this file to .env and fill in your actual values

# Expo Configuration
EXPO_TOKEN=your_expo_access_token_here

# Mapbox Configuration
MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here
MAPBOX_DOWNLOAD_TOKEN=your_mapbox_download_token_here

# AWS Configuration
AWS_REGION=us-east-1
AWS_USER_POOL_ID=your_cognito_user_pool_id
AWS_USER_POOL_CLIENT_ID=your_cognito_client_id
AWS_S3_BUCKET=your_s3_bucket_name
AWS_API_GATEWAY_URL=https://your-api-gateway-url.amazonaws.com

# Monitoring & Analytics
SENTRY_DSN=your_sentry_dsn_here
AMPLITUDE_API_KEY=your_amplitude_api_key_here

# Development Configuration
NODE_ENV=development
DEBUG_MODE=true

# Security Configuration
ENCRYPTION_KEY=your_32_character_encryption_key_here
API_BASE_URL=https://api.trailrun.app

# Optional: Third-party Services
WEATHER_API_KEY=your_weather_api_key_here
ELEVATION_API_KEY=your_elevation_api_key_here